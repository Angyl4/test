class Animal:
    def _init_(self, name, data):
        self.data = data
        self.name = name

    def _str_(self):
        return f"{self.name} – издает звук {self.data}"

animal1 = Animal('Cat', 'мяу')

animal2 = Animal('Dog', 'гав')

animal3 = Animal('Cow', 'му') 


print(animal1)
print(animal2)
print(animal3)