from aiogram import Bot, Dispatcher, executor, types
from aiogram.types.web_app_info import WebAppInfo
import settings5

bot = Bot(settings5.API_KEY)
dp = Dispatcher(bot)

@dp.message_handler(commands=['start'])
async def start(message: types.Message):
    markup = types.ReplyKeyboardMarkup()
    markup.add(types.KeyboardButton('Открыть website', web_app=WebAppInfo(url='https://webapp/index.html'))) #указываем ссылку на веб приложение
    await message.answer('Привет хакер', reply_markup=markup)

executor.start_polling(dp)